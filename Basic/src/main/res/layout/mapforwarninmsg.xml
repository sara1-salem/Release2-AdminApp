<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddWarningMsg">

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="418dp"
        android:layout_height="800dp"
        app:layout_constraintBottom_toTopOf="@+id/relativeLayout4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:uiRotateGestures="true"
        app:uiScrollGestures="true"
        app:uiTiltGestures="true"
        app:uiZoomGestures="true" />


    <RelativeLayout
        android:id="@+id/relativeLayout4"
        android:layout_width="410dp"
        android:layout_height="71dp"
        android:background="#00000000"
        android:backgroundTint="#00000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">


        <Button
            android:id="@+id/editDeleteWM"
            android:layout_width="193dp"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_marginStart="0dp"
            android:layout_marginEnd="4dp"
            android:layout_toStartOf="@+id/DeleteWM"
            android:background="@drawable/clsc_btn_for_all"
            android:drawableTop="@drawable/edit_ic_96"
            android:onClick="OnEditMSGBtn"
            android:padding="10dp"
            android:shadowColor="@color/white"
            android:text="Edit Warning Messages"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/DeleteWM"
            android:layout_width="213dp"
            android:layout_height="match_parent"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="0dp"
            android:background="@drawable/clsc_btn_for_all"
            android:drawableTop="@drawable/icons8_delete_96"
            android:onClick="OnDeleteMSGBtn"
            android:padding="10dp"
            android:shadowColor="@color/white"
            android:text="Delete Warning Messages"
            android:textAllCaps="false" />
    </RelativeLayout>

    <Button
        android:id="@+id/button2"
        android:layout_width="105dp"
        android:layout_height="54dp"
        android:layout_margin="5dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/bcbtn"
        android:drawableStart="@drawable/back_button_blu"
        android:onClick="OnBackBtnClick"
        android:padding="10dp"
        android:text="Return"
        android:textAlignment="viewStart"
        android:textAllCaps="false"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>